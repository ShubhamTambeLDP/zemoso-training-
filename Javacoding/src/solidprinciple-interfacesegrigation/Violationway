public interface Animals  {
    void feed();
    void play();
    void farming();
    void nightMonitoring();

    
}
public class Dogs implements Animals {

    public void feed (){
       System.out.println("i can eat");
   
   }
   public void play (){
       System.out.println("i can play");
   }
   public void farm (){
       throw new AssertionError("i can't do farmimng");
   }
   public void nightMonitoring(){
    System.out.println("i can do montoring in night");


   }}

    
   
   // Violation of solid priciple Interface segrigation
   
   